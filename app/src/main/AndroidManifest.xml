<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.sikogrup.level_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_levelapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginGM" />
        <activity android:name=".RegisGM" />
        <activity android:name=".MainMenuGM" />
        <activity android:name=".LoginAdv" />
        <activity android:name=".RegisAdv" />
        <activity android:name=".MainMenuAdv" />
        <activity android:name=".ShowGuildGM" />
        <activity android:name=".ShowGuildAdv" />
        <activity android:name=".GuildCreate" />
        <activity android:name=".GmGuildDetail" />
        <activity android:name=".CreateQuest" />
        <activity android:name=".AdvGuildDetail"></activity>
    </application>

</manifest>